{% extends '@FormBuilder/layout_funnel.html.twig' %}

{% if formThemePath is not null %}
    {% form_theme form formThemePath %}
{% endif %}

{% block funnel_content %}

    {{ form_start(form, {attr: {'novalidate': 'novalidate'}}) }}

    {{ form_errors(form) }}

    {% if renderType == 'include' %}
        {% include view %}
    {% elseif renderType == 'prerender' %}
        {{ view|raw }}
    {% endif %}

    {% if form._token is defined and not form._token.isRendered %}
        {{ form_widget(form._token) }}
    {% endif %}

    {{ form_end(form, {render_rest: formRenderRest}) }}

{% endblock funnel_content %}